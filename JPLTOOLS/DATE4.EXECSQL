
  DROP
       FUNCTION &JPLTOOLS.X2DATE(
       VARCHAR (10)
  )
monmsg *ignore
EXECUTESQL

   CREATE FUNCTION &JPLTOOLS.X2DATE(
     ALPHA VARCHAR (10)
     )
     RETURNS DATE
     LANGUAGE SQL
     SPECIFIC &JPLTOOLS.X2DATE
     NOT DETERMINISTIC
     MODIFIES SQL DATA
     CALLED ON NULL INPUT
   BEGIN
   RETURN &JPLTOOLS.X2DATE ( ALPHA , '*ISO'  );
   END
monmsg ST01660
executesql
