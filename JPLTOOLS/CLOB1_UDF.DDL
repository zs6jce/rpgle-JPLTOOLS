    DROP   FUNCTION JPLSQL.CLOB1_UDF
;

    CREATE FUNCTION JPLSQL.CLOB1_UDF(
-- NO INPUT PARAMETER
    )
    RETURNS
    CLOB(4M) AS LOCATOR

      LANGUAGE RPGLE     PARAMETER STYLE DB2SQL
      NO FINAL CALL      DISALLOW PARALLEL
      NO DBINFO          SCRATCHPAD 500
      EXTERNAL NAME  JPLSQL.CLOB1(CLOB1_P1)
      SPECIFIC JPLSQL.CLOB1SP
      NOT DETERMINISTIC
      READS SQL DATA
      CALLED ON NULL INPUT
      EXTERNAL ACTION


