EXECOPT SQLOPT=*GEN

  DROP
       FUNCTION &JPLTOOLS.N2DATE(
       BIGINT
  )
monmsg *ignore
EXECUTESQL

CREATE FUNCTION &JPLTOOLS.N2DATE(
  NUM  BIGINT
  )
  RETURNS DATE
  LANGUAGE SQL
  SPECIFIC &JPLTOOLS.N2DATE
  NOT DETERMINISTIC
  MODIFIES SQL DATA
  CALLED ON NULL INPUT
BEGIN
RETURN  &JPLTOOLS.N2DATE ( NUM , '*ISO' ) ;
END
monmsg ST01660
EXECUTESQL
